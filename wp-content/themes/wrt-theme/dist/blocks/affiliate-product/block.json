{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "title": "Affiliate Product",
  "description": "Affiliate Product",
  "textdomain": "wrt-theme",
  "name": "wrt/affiliate-product",
  "icon": "index-card",
  "category": "formatting",
  "attributes":{
    "productPostId": {
      "type": "integer",
      "default": 0
